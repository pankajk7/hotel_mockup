					e.printStackTrace();
				}
				
				try {
					if (success.equalsIgnoreCase("True")) {
						onSuccess(data, success);
					}else{
						ITZipzError objItZipzError = new ITZipzError(
								objResponseHeader);
						onSuccess(objItZipzError);
					}
				}catch(Exception e){
					e.printStackTrace();
				}
				super.success(data, response);
			}

			@Override
			public void complete() {
				onComplete();
				super.complete();
			}

			@Override
